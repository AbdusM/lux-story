{
  "name": "lux-story",
  "version": "2.2.0",
  "type": "module",
  "description": "Performance-based career exploration platform using contemplative storytelling and behavioral psychology",
  "main": "index.js",
  "scripts": {
    "dev": "PORT=3005 next dev",
    "prebuild": "node scripts/validate-build-config.cjs",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:validators": "vitest run tests/lib/dialogue-validators.test.ts tests/lib/simulation-validators.test.ts tests/lib/simulation-id-drift.test.ts tests/lib/simulation-entry-nodes.test.ts tests/lib/choice-generator.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "playwright:install": "PLAYWRIGHT_BROWSERS_PATH=0 playwright install",
    "playwright:install:deps": "PLAYWRIGHT_BROWSERS_PATH=0 playwright install --with-deps",
    "qa": "node --loader ./scripts/ts-loader.mjs scripts/run-qa.ts",
    "qa:quick": "npm run type-check && npm run test:run",
    "verify": "npm run type-check && npm run lint && npm run test:run && rm -rf .next && npm run build",
    "validate-graphs": "node --loader ./scripts/ts-loader.mjs scripts/validate-dialogue-graphs.ts",
    "validate-content": "node --loader ./scripts/ts-loader.mjs scripts/validate-content-integrity.ts",
    "analyze-text-density": "node --loader ./scripts/ts-loader.mjs scripts/analyze-text-density.ts",
    "validate-skills": "node --loader ./scripts/ts-loader.mjs scripts/validate-skills-system.ts",
    "validate-env": "node --loader ./scripts/ts-loader.mjs scripts/validate-env.ts",
    "verify:admin": "node --loader ./scripts/ts-loader.mjs scripts/verify-production-admin.ts",
    "verify:admin:prod": "node --loader ./scripts/ts-loader.mjs scripts/verify-production-admin.ts",
    "test:admin-improvements": "node --loader ./scripts/ts-loader.mjs scripts/test-admin-improvements.ts",
    "verify:env:vercel": "vercel env ls",
    "predeploy": "npm run validate-env && npm run test:run && npm run build",
    "deploy": "npx wrangler pages deploy out --project-name=lux-story",
    "deploy:vercel": "npm run validate-env && vercel --prod",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "clean": "rm -rf .next out node_modules/.cache",
    "analyze-typewriter": "node scripts/gemini-typewriter-analysis.js",
    "check-typewriter": "node scripts/check-typewriter-compliance.js",
    "analyze-chat-master": "node scripts/gemini-chat-interface-master-analyzer.js",
    "analyze-skills-dashboard": "node scripts/gemini-skills-dashboard-analyzer.js",
    "audit-config": "node --loader ./scripts/ts-loader.mjs scripts/audit-configuration.ts",
    "generate-coverage-matrix": "node --loader ./scripts/ts-loader.mjs scripts/generate-coverage-matrix.ts",
    "report-entry-nodes": "node --loader ./scripts/ts-loader.mjs scripts/report-entry-nodes.ts",
    "precommit": "npm run check-typewriter && npm run lint",
    "health-check": "curl http://localhost:3005/api/health && curl http://localhost:3005/api/health/db",
    "validate-simulations-doc": "node --loader ./scripts/ts-loader.mjs scripts/validate-simulations-doc.ts",
    "verify:character-coverage": "node --loader ./scripts/ts-loader.mjs scripts/verify-character-coverage.ts",
    "verify:required-state-guarding": "node --loader ./scripts/ts-loader.mjs scripts/verify-required-state-guarding.ts",
    "verify:required-state-strict": "node --loader ./scripts/ts-loader.mjs scripts/verify-required-state-strict.ts",
    "verify:arc-completion-flags": "node --loader ./scripts/ts-loader.mjs scripts/verify-arc-completion-flags.ts",
    "verify:narrative-sim": "node --loader ./scripts/ts-loader.mjs scripts/verify-narrative-sim.ts",
    "verify:unreferenced-dialogue-nodes": "node --loader ./scripts/ts-loader.mjs scripts/verify-unreferenced-dialogue-nodes.ts",
    "verify:unreachable-dialogue-nodes": "node --loader ./scripts/ts-loader.mjs scripts/verify-unreachable-dialogue-nodes.ts",
    "verify:content-quarantine": "NEXT_PUBLIC_INCLUDE_DRAFT_CONTENT=true node --loader ./scripts/ts-loader.mjs scripts/verify-unreachable-dialogue-nodes.ts && node --loader ./scripts/ts-loader.mjs scripts/verify-quarantine-list.ts",
    "write:content-quarantine": "node --loader ./scripts/ts-loader.mjs scripts/write-quarantine-list.ts",
    "check:core-contracts": "node scripts/check-core-gameplay-contracts.js",
    "audit:core-diff": "node scripts/report-core-gameplay-diff.js",
    "verify:analytics-dict": "node --loader ./scripts/ts-loader.mjs scripts/verify-analytics-dictionary.ts && node --loader ./scripts/ts-loader.mjs scripts/verify-no-inmemory-analytics.ts",
    "verify:reserved-choice-ids": "node --loader ./scripts/ts-loader.mjs scripts/verify-reserved-choice-ids.ts",
    "verify:validate-graphs-info": "node --loader ./scripts/ts-loader.mjs scripts/verify-validate-graphs-info-ratchet.ts"
  },
  "keywords": [
    "career-exploration",
    "contemplative-gaming",
    "performance-psychology",
    "adaptive-narrative",
    "birmingham"
  ],
  "author": "Lux Story Team",
  "license": "MIT",
  "homepage": "https://lux-story.pages.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdusM/lux-story.git"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-hook/resize-observer": "^2.0.2",
    "@react-pdf/renderer": "^4.3.1",
    "@sentry/nextjs": "^10.17.0",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/d3": "^7.4.3",
    "@types/d3-force": "^3.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.539.0",
    "next": "^15.5.7",
    "playwright": "^1.55.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.6.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "^15.5.3",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.12",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "jsdom": "^27.2.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "puppeteer": "^24.22.3",
    "supawright": "^0.4.2",
    "ts-morph": "^27.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead",
    "iOS >= 14",
    "Android >= 8"
  ]
}
